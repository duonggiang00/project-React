{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
   
    {
      "src": "/api/db.json",
      "dest": "/db.json",
      "headers": {
        "Content-Type": "application/json"
      }
    }
  ]
}